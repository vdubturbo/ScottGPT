{
  "name": "scottgpt",
  "version": "1.0.0",
  "description": "An Interactive AI-powered resume that answers questions about professional experience",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "npx concurrently \"nodemon server.js\" \"cd client && npm start\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint . --ignore-path .eslintignore",
    "ingest": "./scripts/ingest.sh",
    "ingest:normalize": "node scripts/normalize.js",
    "ingest:extract": "node scripts/extract.js",
    "ingest:validate": "node scripts/validate.js",
    "ingest:write": "node scripts/write.js",
    "ingest:index": "node scripts/indexer.js",
    "archive": "node scripts/archive-processed.js",
    "import:manual": "node scripts/manual-import.js",
    "tags:report": "node scripts/tag-manager.js report",
    "tags:approve": "node scripts/tag-manager.js approve",
    "tags:reject": "node scripts/tag-manager.js reject",
    "skills:report": "node scripts/skill-manager.js report",
    "skills:approve": "node scripts/skill-manager.js approve",
    "skills:approve-high": "node scripts/skill-manager.js approve-high",
    "skills:approve-category": "node scripts/skill-manager.js approve-category",
    "skills:interactive": "node scripts/skill-manager.js interactive"
  },
  "keywords": [
    "ai",
    "resume",
    "rag",
    "chatgpt",
    "interactive"
  ],
  "author": "Scott Lovett",
  "license": "MIT",
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "@supabase/supabase-js": "^2.39.0",
    "cohere-ai": "^7.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "gray-matter": "^4.0.3",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.24.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
