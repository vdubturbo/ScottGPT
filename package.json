{
  "name": "scottgpt",
  "version": "1.0.0",
  "description": "An Interactive AI-powered resume that answers questions about professional experience",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint .",
    "ingest": "./scripts/ingest.sh",
    "ingest:normalize": "node scripts/normalize.js",
    "ingest:extract": "node scripts/extract.js",
    "ingest:validate": "node scripts/validate.js",
    "ingest:write": "node scripts/write.js",
    "ingest:index": "node scripts/indexer.cjs"
  },
  "keywords": ["ai", "resume", "rag", "chatgpt", "interactive"],
  "author": "Scott Lovett",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.24.1",
    "cohere-ai": "^7.7.0",
    "@supabase/supabase-js": "^2.39.0",
    "helmet": "^7.1.0",
    "execa": "^8.0.1",
    "gray-matter": "^4.0.3",
    "@sindresorhus/slugify": "^2.2.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}